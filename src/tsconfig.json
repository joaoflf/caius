{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "mapRoot": "",
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "outDir": "../dist/",
    "rootDir": ".",
    "sourceMap": true,
    "target": "es5",
    "inlineSources": true
  },
  "filesGlob": [
    "**/*.ts",
    "!../node_modules/**",
    "!../typings/**/*.ts"
  ],
  "files": [
    "app/caius.component.spec.ts",
    "app/caius.component.ts",
    "app/common/index.ts",
    "app/common/interfaces.ts",
    "app/common/reducers.ts",
    "app/environment.ts",
    "app/header/header.component.spec.ts",
    "app/header/header.component.ts",
    "app/header/index.ts",
    "app/index.ts",
    "app/item-container/index.ts",
    "app/item-container/item-container.component.spec.ts",
    "app/item-container/item-container.component.ts",
    "app/item/item.actions.ts",
    "app/item/item.model.ts",
    "app/item/item.reducers.ts",
    "app/main-container/index.ts",
    "app/main-container/main-container.component.spec.ts",
    "app/main-container/main-container.component.ts",
    "app/main-container/main-container.service.spec.ts",
    "app/main-container/main-container.service.ts",
    "app/sidebar/index.ts",
    "app/sidebar/sidebar.component.spec.ts",
    "app/sidebar/sidebar.component.ts",
    "main.ts",
    "system-config.ts",
    "typings.d.ts"
  ],
  "atom": {
    "rewriteTsconfig": true
  }
}
